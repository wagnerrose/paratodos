<% if ! notice.blank? %>
  <div class="alert alert-info alert-dismissible" role="alert">
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span></button>
      <strong id="notice"><%= notice %></strong>
  </div>
<% end %>
<% if ! localidades.nil? %>
  <table class="table display">
    <thead>
        <th>Localidade</th>
        <th>Endere√ßo</th>
        <th>Latitude</th>
        <th>Longitude</th>
    </thead>

    <tbody>
      <% localidades.each do |local| %>
        <tr>
          <td><%= local.neighborhood %></td>
          <td><%= local.address %></td>
          <td><%= local.latitude %></td>
          <td><%= local.longitude %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>
